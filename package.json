{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "sh -c 'mkdir -p .logs && vercel --prod 2>&1 | tee .logs/deploy.log'",
    "deploy:last": "cat .logs/deploy.log",
    "cli:build": "npm --prefix tools/websitectl-ts run build",
    "cli:install": "npm --prefix tools/websitectl-ts run install:global",
    "cli:publish": "npm --prefix tools/websitectl-ts run publish:npm",
    "email:test": "dotenv -e .env.local -- sh -c 'curl -s -X POST http://localhost:3000/api/email/send-test -H \"Authorization: Bearer $CRON_SECRET\" -H \"Content-Type: application/json\"' | jq"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.24",
    "@headlessui/react": "^1.7.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@react-email/components": "^1.0.8",
    "@react-email/tailwind": "^2.0.5",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.44.7",
    "framer-motion": "^8.0.2",
    "next": "^14.0.0",
    "postgres": "^3.4.7",
    "posthog-js": "^1.289.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-markdown": "^10.1.0",
    "react-quill": "^2.0.0",
    "resend": "^6.9.2",
    "tailwind-merge": "^3.3.1",
    "turndown": "^7.2.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.7",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "^5.0.0"
  }
}
